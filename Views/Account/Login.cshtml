@{ 
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<form method="post" asp-action="Login">
    Email <input name="email" type="text" /><p></p>
    Password <input name="password" type="text" /><p></p>
    <button type="submit">Log In</button>

</form>




